<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <item
        android:state_first="true"
        android:state_pressed="true"
        android:drawable="@drawable/background_round_corner_header_pressed" />
    
    <item
        android:state_first="true"
        android:state_pressed="false"
        android:drawable="@drawable/background_round_corner_header_unpress" />
    
    <item
        android:state_middle="true"
        android:state_pressed="true"
        android:drawable="@drawable/background_round_corner_middle_pressed" />
    
    <item
        android:state_middle="true"
        android:state_pressed="false"
        android:drawable="@drawable/background_round_corner_middle_unpress" />
    
    <item
        android:state_last="true"
        android:state_pressed="true"
        android:drawable="@drawable/background_round_corner_footer_pressed" />
    
    <item
        android:state_last="true"
        android:state_pressed="false"
        android:drawable="@drawable/background_round_corner_footer_unpress" />
    
    <item
        android:state_single="true"
        android:state_pressed="true"
        android:drawable="@drawable/background_round_corner_single_pressed" />
    
    <item
        android:state_single="true"
        android:state_pressed="false"
        android:drawable="@drawable/background_round_corner_single_unpress" />
</selector>
